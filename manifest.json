{
    "manifest_version": 3,
    "name": "Hello Extensions",
    "description": "Base Level Extension",
    "version": "1.0",
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
        "matches": [
            "*://mail.google.com/mail/*"
        ],
        "js": [
            "script.js"
        ],
        "css": [
            "style.css"
        ],
        "all_frames": true
        }
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "logo.png"
    }
}